{
  "name": "simple-stack-form",
  "version": "0.0.6",
  "description": "A simple form library for Astro projects",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "tsc --project tsconfig.json --watch"
  },
  "exports": {
    ".": {
      "default": "./src/index.mjs",
      "types": "./src/index.d.ts"
    },
    "./internal/*": "./dist/*",
    "./middleware": "./src/middleware.ts",
    "./client": "./src/client.ts",
    "./components": "./components/index.ts",
    "./types": "./types.d.ts"
  },
  "keywords": [
    "withastro",
    "astro-integration"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bholmesdev/simple-stack.git",
    "directory": "packages/form"
  },
  "author": "bholmesdev",
  "license": "MIT",
  "devDependencies": {
    "@nanostores/react": "^0.7.1",
    "@types/node": "^20.10.4",
    "@types/react": "^18.0.21",
    "astro": "^4.0.0",
    "nanostores": "^0.9.5",
    "react": "^18.0.0",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "astro": "^3.6.0 || ^4.0.0",
    "nanostores": "^0.9.5",
    "zod": "^3.22.4"
  },
  "peerDependenciesMeta": {
    "nanostores": {
      "optional": true
    }
  },
  "dependencies": {
    "just-map-object": "^2.3.0"
  }
}
