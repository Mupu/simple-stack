---
const result = await Astro.locals.suspend(() => Astro.slots.render("default"));
---

{result.render === "content" && <Fragment set:html={result.value} />}
{
  result.render === "fallback" && (
    <div style="display: contents" data-suspense-fallback={result.id}>
      <slot name="fallback" />
    </div>
  )
}
